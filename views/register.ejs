        <h4 class="h4 text-secondary">Personal Data</h4>
        <hr class="bg-dark border-2 border-top border-dark mt-0">
        <div class="form-floating mb-1">
          <input name="username" type="text" class="form-control" id="floatingInputName" placeholder="username"
            autocomplete="off">
          <label for="floatingInput">Username</label>
        </div>
        <div class="form-floating mb-1">
          <input name="email" type="email" class="form-control" id="email-register" placeholder="name@example.com"
            autocomplete="off">
          <label for="email-register">Email address</label>
        </div>
        <div class="form-floating  mb-3">
          <input name="password" type="password" class="form-control" id="password-register" placeholder="Password">
          <label for="password-register">Password</label>
        </div>
